<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>ControllerTrabajador</servlet-name>
        <servlet-class>Controladores.ControllerTrabajador</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControllerMensualidad</servlet-name>
        <servlet-class>Controladores.ControllerMensualidad</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControllerLogin</servlet-name>
        <servlet-class>Controladores.ControllerLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControllerLogout</servlet-name>
        <servlet-class>Controladores.ControllerLogout</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ControllerTrabajador</servlet-name>
        <url-pattern>/Trabajador/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControllerMensualidad</servlet-name>
        <url-pattern>/Mensualidad/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControllerLogin</servlet-name>
        <url-pattern>/ControllerLogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControllerLogout</servlet-name>
        <url-pattern>/ControllerLogout</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>login.jsp</welcome-file>
    </welcome-file-list>
</web-app>
