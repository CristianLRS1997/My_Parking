<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC "-//Hibernate/Hibernate Configuration DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
  <session-factory>
    <property name="hibernate.dialect">org.hibernate.dialect.MySQLDialect</property>
    <property name="hibernate.connection.driver_class">com.mysql.jdbc.Driver</property>
    <property name="hibernate.connection.url">jdbc:mysql://localhost:3306/my_parking</property>
    <property name="hibernate.connection.username">root</property>
    <mapping resource="Mapeos/Parqueadero.hbm.xml"/>
    <mapping resource="Mapeos/Puesto.hbm.xml"/>
    <mapping resource="Mapeos/Trabajador.hbm.xml"/>
    <mapping resource="Mapeos/Mensualidad.hbm.xml"/>
    <mapping resource="Mapeos/Cliente.hbm.xml"/>
    <mapping resource="Mapeos/Reserva.hbm.xml"/>
  </session-factory>
</hibernate-configuration>
